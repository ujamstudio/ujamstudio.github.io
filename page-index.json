---
layout: null
---
[
  {% assign all_pages = site.posts | concat: site.knowledge %}
  {% for page in all_pages %}
  {
    "title": {{ page.title | jsonify }},
    "url": {{ page.url | relative_url | jsonify }},
    "slug": {{ page.slug | jsonify }}
  }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
